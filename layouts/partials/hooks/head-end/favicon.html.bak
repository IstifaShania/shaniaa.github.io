<!-- Custom Favicon Links -->
<!-- Clear any existing favicon from browser cache -->
<link rel="icon" href="data:,">
<!-- Use direct path with nanosecond precision to force cache reloading -->
<link rel="icon" type="image/png" sizes="16x16" href="/media/favicon.png?v={{ now.UnixNano }}">
<link rel="icon" type="image/png" sizes="32x32" href="/media/favicon.png?v={{ now.UnixNano }}">
<link rel="apple-touch-icon" sizes="180x180" href="/media/favicon.png?v={{ now.UnixNano }}">
<link rel="shortcut icon" href="/media/favicon.png?v={{ now.UnixNano }}">
<meta name="msapplication-TileImage" content="/media/favicon.png?v={{ now.UnixNano }}">
<!-- End Custom Favicon Links -->
